{
  "name":"markocupic/import-from-csv-bundle",
  "description":"Import datarecords from csv-textfile. Backend extension for the Contao Open Source CMS",
  "keywords":["contao", "import from csv"],
  "type":"contao-bundle",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic"
    }
  ],
  "require": {
    "contao/core-bundle":"~3.2 || ~4.4",
    "contao-community-alliance/composer-plugin":"~2.4 || ~3.0" 
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\ImportFromCsvBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config",
      "src/Resources/contao/dca",
      "src/Resources/contao/languages",
      "src/Resources/contao/manual"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\ImportFromCsvBundle\\ContaoManager\\Plugin"
  }
}
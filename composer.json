{
  "name": "markocupic/import-from-csv-bundle",
  "description": "Backend extension for the Contao Open Source CMS. Import datarecords from a CSV spreadsheet.",
  "keywords": [
    "contao",
    "import from csv",
    "spreadsheet"
  ],
  "type": "contao-module",
  "license": "MIT",
  "authors": [
    {
      "name": "Marko Cupic",
      "homepage": "https://github.com/markocupic/import-from-csv-bundle"
    }
  ],
  "require": {
    "php": "^7.3||^8.0",
    "contao/core-bundle": "^4.9",
    "league/csv": "^9.1"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^5.0",
    "contao/manager-plugin": "^2.3.1",
    "contao/test-case": "^4.6",
    "phpunit/phpunit": "^9.3",
    "symfony/phpunit-bridge": "^5.4"
  },
  "conflict": {
    "doctrine/dbal": "<2.11"
  },
  "autoload": {
    "psr-4": {
      "Markocupic\\ImportFromCsvBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "src/Resources/config",
      "src/Resources/dca",
      "src/Resources/languages"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Markocupic\\ImportFromCsvBundle\\ContaoManager\\Plugin"
  }
}

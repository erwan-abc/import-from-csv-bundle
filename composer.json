{
    "name":"markocupic/import-from-csv-bundle",
    "description":"Import datarecords from csv-textfile. Backend extension for the Contao Open Source CMS",
    "keywords":["contao", "Import from csv"],
    "type":"contao-module",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Marko Cupic",
            "homepage": "https://github.com/markocupic"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "contao/core-bundle": ">4.2"
    },
    "autoload": {
        "psr-4": {
            "Markocupic\\ImportFromCsvBundle\\": ""
        },
        "classmap": [
            "Resources/contao"
        ],
        "exclude-from-classmap": [
            "Resources/contao/config",
            "Resources/contao/dca",
            "Resources/contao/languages",
            "Resources/contao/manual"
        ]
    }
}
